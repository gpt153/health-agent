[run]
source = src
omit =
    */tests/*
    */migrations/*
    */__init__.py
    */config.py
    */.venv/*
    */venv/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract
    @abstractmethod
    class .*\bProtocol\):
    @overload

precision = 2
show_missing = True

[html]
directory = htmlcov
